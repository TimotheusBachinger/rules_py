load("//py:defs.bzl", "py_binary", "py_test", rules_py_py_library = "py_library")

rules_py_py_library(
    name = "init",
    srcs = [
        "__init__.py",
    ],
    tags = ["skip-typecheck"],
)

rules_py_py_library(
    name = "sub",
    srcs = [
        "sub.py",
    ],
    tags = ["skip-typecheck"],
    deps = [
        ":init",
    ],
)

py_library(
    name = "pi",
    srcs = [
        "pi.py",
    ],
    tags = ["skip-typecheck"],
    deps = [
        ":init",
    ],
)

py_binary(
    name = "main",
    srcs = [
        "__main__.py",
    ],
    tags = ["skip-typecheck"],
    deps = [
        ":pi",
        ":sub",
        "//py/tests/internal-deps/adder",
    ],
)

py_test(
    name = "assert",
    srcs = [
        "__main__.py",
    ],
    tags = ["skip-typecheck"],
    deps = [
        ":pi",
        ":sub",
        "//py/tests/internal-deps/adder",
    ],
)
